<div class="content">
    <div class="container-fluid">
        <div class="row">
          <div class="col-md-6">
            <div class="card ">
              <div class="header">
                <h4 class="title">Latests Leads</h4>
                <p class="category">Potential Contacts</p>
              </div>
              <div class="content">
                <div class="table-full-width">
                  <table class="table">
                    <thead>
                        <th>Email</th>
                          <th>Name</th>
                            <th>Requests</th>
                            <th>Country</th>


                    </thead>

                    <tbody>
                        <% @contacts.each do |c| %>
                        <tr>
                          <td>
                          <%= c.email %>
                          </td>
                          <td>
                          <%= c.name %>
                          </td>
                          <td>
                            <%= link_to c.ideaname, managements_contact_path(c) %>

                          </td>
                          <td>
                          <%= c.country %>
                          </td>
                        </tr>

                      <% end %>
                    </tbody>
                  </table>
                </div>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
